{
  "name": "nesttest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist && yarn copy:templates",
    "build": "nest build --watch --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "yarn copy:templates",
    "start": "yarn start:debug",
    "start:debug": "nest start --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy:templates": "cpx 'src/templates/**' 'dist/templates'",
    "mockdb": "./mockdb.sh"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@nest-modules/mailer": "^1.3.9",
    "@nestjs/common": "^7.0.1",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.1",
    "@nestjs/graphql": "^7.0.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-fastify": "^7.0.1",
    "@nestjs/platform-socket.io": "^7.0.1",
    "@nestjs/websockets": "^7.0.1",
    "@typegoose/typegoose": "^6.4.0",
    "accesscontrol": "^2.2.1",
    "apollo-server-fastify": "^2.11.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "file-type": "^14.1.4",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "mongoose": "^5.9.4",
    "nanoid": "^2.1.11",
    "nest-access-control": "^2.0.1",
    "nestjs-command": "^1.3.0",
    "nestjs-role-protected": "^0.1.3",
    "nestjs-typegoose": "^7.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.27.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-rokket-labs": "^0.1.4",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-tag-pluck": "^0.8.7",
    "jest": "^25.1.0",
    "mockingoose": "^2.13.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
